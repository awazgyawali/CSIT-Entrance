<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.other_fragments.YearsList">


    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/work_sans_medium"
            android:padding="10dp"
            android:text="College Model Questions"
            android:textColor="@color/colorPrimary"
            android:textSize="17sp"
            android:visibility="gone"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <np.com.aawaz.csitentrance.custom_views.CollegeModelCard
            android:id="@+id/collegeModel1"
            android:layout_width="match_parent"
            android:visibility="gone"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.CollegeModelCard
            android:id="@+id/collegeModel2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" />

        <np.com.aawaz.csitentrance.custom_views.CollegeModelCard
            android:id="@+id/collegeModel3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/work_sans_medium"
            android:padding="10dp"
            android:text="Old Questions"
            android:textColor="@color/colorPrimary"
            android:textSize="17sp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2069"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2070"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2071"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2072"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2073"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/question2074"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/work_sans_medium"
            android:padding="10dp"
            android:text="Model Questions"
            android:textColor="@color/colorPrimary"
            android:textSize="17sp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <np.com.aawaz.csitentrance.custom_views.YearCard
            android:id="@+id/model6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
